{
  "hosting": {
    "public": "dist/test3",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/nl/**",
        "destination": "/nl/index.html"
      },
      {
        "source": "/en/**",
        "destination": "/en/index.html"
      },
      {
        "source": "/sr/**",
        "destination": "/sr/index.html"
      }
    ],
    "i18n": {
      "root": "/"
    }
  }
}